{
  "name": "trailnet",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite --config frontend/vite.config.ts",
    "dev:backend": "nodemon backend/server.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build:frontend": "vite build --mode production --config frontend/vite.config.ts",
    "build:backend": "echo \"No build step for backend\"",
    "start:frontend": "serve -s dist",
    "start:backend": "node backend/server.js"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.0",
    "nodemailer": "^6.9.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.0",
    "serve": "^6.5.8"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "nodemon": "^3.1.4",
    "typescript": "^4.9.3",
    "vite": "^4.5.5"
  }
}
