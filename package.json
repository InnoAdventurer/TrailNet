{
  "name": "trailnet",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://innoadventurer.github.io/TrailNet/",
  "scripts": {
    "dev:frontend": "vite --config frontend/vite.config.ts",
    "dev:backend": "nodemon backend/server.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "node backend/server.js"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "gh-pages": "^6.1.1",
    "nodemon": "^3.1.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
